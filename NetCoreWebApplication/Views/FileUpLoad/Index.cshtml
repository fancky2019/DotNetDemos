


<div class="row">
    <div class="col-md-12">


        <div id="top" class="tile" style="height: 70px">
            <div style="display: inline-block">
                @*默认单选  multiple="false"*@  @*multiple="multiple"*@
                <input type="file" id="uploadFile" style="display: none;" accept=".*" />
                <input id="selectFilePath" readonly>
                <input id="uploadPath" type="hidden">
                <input type="button" id="openFile" value="浏览">
                <input id="upload" type="button" value="上传" />



            </div>



            <!--margin:上右下左-->
            <div style="display: inline-block;width: 200px;">
                <div class="progress" style="margin-bottom: 0px;">
                    <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"
                         style="width: 0%;">
                        0%
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>
<script src="js/jquery-3.3.1.min.js"></script>
<script src="~/js/fileupload.js"></script>
